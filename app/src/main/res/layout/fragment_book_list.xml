    <TextView
        android:id="@+id/tv4"
        android:textSize="@dimen/dimeFragBook"
        tools:text="@tools:sample/full_names"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tv5"
        android:textSize="@dimen/dimeFragBook"
        tools:text="@tools:sample/full_names"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
